{
  "compilerOptions": {
    "target": "es2021",
    "module": "commonjs",
    "outDir": "./build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "baseUrl": ".", 
    "paths": {
      "@routes/*":["src/routes/*"],
    },
    "rootDir": "./src",
  }
}
